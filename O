#include <iostream>
using namespace std;

int main()
{
    int A, B, R;
	char S;
	cin >> A >> S >> B;
	if (S=='+')
	{
		R = A + B;
		cout << R;
	}
	else if (S=='-')
	{
		R = A - B;
		cout << R;
	}
	else if (S=='*')
	{
		R = A * B;
		cout << R;
	}
	else if (S=='/')
	{
		R = A / B;
		cout << R;
	}
	return 0;
}
